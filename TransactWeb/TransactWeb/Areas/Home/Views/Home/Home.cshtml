@{
    ViewBag.Title = "Home";
}



<script type="text/javascript">


    $(document).ready(function () {

        $("#MenuItemChecks").addClass("MenuItemSelected");

        initializeMenu();
        initialTab();
    });


    function initializeMenu() {

        $("#TransactWebMenu").kendoMenu({

            animation: {
                open: { effects: "fadeIn" },
                hoverDelay: 500
            }

           // select: OnMenuItemClick
        });
    }
    

    function initialTab() {

        $("#tabStrip").kendoTabStrip();
    }



</script>